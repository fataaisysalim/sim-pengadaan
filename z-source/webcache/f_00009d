<script type="text/javascript">
    $(".loadertab").hide();
    $(".edits").click(function() {
        $(this).each(function() {
            $(".alert").hide('slow');
            $("#saldoform").html('<div class="loader mg-t"><i class="fa fa-spin fa-refresh mg-r-md"></i>Loading data. Please wait... !</div>');
            var id = $(this).attr('id');
            $("#message").hide('slow');
            $("#pesan").hide('slow');
            $("#saldoform").load('http://192.168.1.4/forder-nasiudukpalagan/saldo/form/' + id);
        });
    });
</script>
<script src="http://192.168.1.4/forder-nasiudukpalagan/assets/js/datatables.js"></script>